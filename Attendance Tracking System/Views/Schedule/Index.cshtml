@model IEnumerable<Schedule>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
</head>
<body>
    <table class="table">
        <thead>
            <tr>
                <td>Shedule ID</td>
                <td>Shedule Date</td>
                <td>Shedule StartTime</td>
                <td>Track Name</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Date</td>
                    <td>@item.StartTime</td>
                    <td>@item.Track.Name</td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>
