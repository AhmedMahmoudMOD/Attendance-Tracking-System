@model List<Student>
<table class="table table-striped table-borderless">
    <thead>
        <tr>
            <th>
                ID
            </th>
            <th>
                Name
            </th>
            <th>
                Email
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var std in Model)
        {
            <tr>
                <td>
                    @std.Id
                </td>
                 <td>
                    @std.Name
                </td>
                <td>
                    @std.Email
                </td>
            </tr>
               
        }
    </tbody>
</table>